ord', word_vectorizer)\n","])\n","\n","# Apply the vectorizer to the Summary and Text columns\n","X = vectorizer.fit_transform(df[['Summary', 'Text']].apply(lambda x: ' '.join(x), axis=1))\n","\n","# Apply MaxAbsScaler to the feature matrix\n","scaler = MaxAbsScaler()\n","X_scaled = scaler.fit_transform(X)\n","\n","# Define the base models\n","model1 = lgb.LGBMRegressor(objective='regression', metric='rmse', num_leaves=31, learning_rate=0.05, feature_fraction=0.9)\n","model2 = AdaBoostRegressor(n_estimators=100, learning_rate=0.1)\n","model3 = xgb.XGBRegressor(objective='reg:squarederror', n_estimators=100, max_depth=3, learning_rate=0.1)\n","\n","# Define the stacking ensemble model\n","ensemble_model = StackingRegressor(estimators=[('lgbm', model1), ('ada', model2), ('xgb', model3)], final_estimator=lgb.LGBMRegressor())\n","\n","# Train the stacking ensemble model\n","ensemble_model.fit(X_scaled, df['Score'])\n"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"background_save":true},"id":"aJXrBc4qfL5R"},"outputs":[],"source":["X_submission = pd.read_csv(\"drive/MyDrive/CS506-DS/midterm/data/X_test.csv\")\n","X_submission.drop(columns=[\"ProductId\",\"UserId\",\"Time\",\"Score\"], axis=1, inplace=True)\n","\n","X_submission['Summary'] = X_submission['Summary'].astype(str)\n","X_submission['Text'] = X_submission['Text'].astype(str)\n","\n","new_X=vectorizer.transform(X_submission['Summary'] + ' ' + X_submission['Text'])\n","new_X_scaled = scaler.transform(new_X)\n","\n","X_submission['Score'] = model.predict(new_X_scaled)\n","\n","submission = X_submission[['Id', 'Score']]\n","submission.to_csv(\"submission.csv\", index=False)"]}],"metadata":{"colab":{"authorship_tag":"ABX9TyPUyYnhKWuDneJLIqc40+MG","name":"","version":""},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}